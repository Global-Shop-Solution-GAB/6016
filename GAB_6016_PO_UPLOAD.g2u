	f.Intrinsic.String.Build("SELECT SEQ FROM V_JOB_OPERATIONS WHERE JOB='{0}' AND SUFFIX='{1}' AND PART='{2}'",v.args.Job,v.Args.Suffix,v.Args.Part,v.Local.ssql)
