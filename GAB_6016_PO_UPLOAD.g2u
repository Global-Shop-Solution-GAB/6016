	v.Local.ssql.Declare
	v.Local.fUnits.Declare
	v.Local.iWidth.Declare
	v.Local.iHeight.Declare
	
	f.Intrinsic.String.Mid(v.Args.Content,88,4,v.Local.iWidth)
